def checkPalindrome(num):
    copy=num
    new=0
    while num!=0:
        
        digit=num%10
        new=new*10+digit
        num=num//10
    if new == copy:
        return True
    else:
        return False
num = int(input())
isPalindrome = checkPalindrome(num)
if(isPalindrome):
	print('true')
else:
	print('false')
